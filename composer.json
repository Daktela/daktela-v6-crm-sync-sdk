{
    "name": "daktela/daktela-v6-crm-sync",
    "description": "Universal sync layer between Daktela Contact Centre V6 and any CRM system",
    "type": "library",
    "version": "1.0.0",
    "license": "proprietary",
    "keywords": ["daktela", "crm", "sync", "contact-centre", "integration"],
    "authors": [
        {
            "name": "Daktela",
            "email": "daktela@daktela.com",
            "homepage": "https://www.daktela.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Daktela/daktela-v6-crm-sync-sdk/issues",
        "source": "https://github.com/Daktela/daktela-v6-crm-sync-sdk"
    },
    "require": {
        "php": ">=8.2",
        "daktela/daktela-v6-php-connector": "^2.4",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^2.0||^3.0",
        "psr/http-message": "^2.0",
        "symfony/yaml": "^6.0||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Daktela\\CrmSync\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daktela\\CrmSync\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/crm-inspect", "bin/config-view"],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
